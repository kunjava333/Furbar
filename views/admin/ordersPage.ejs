                <div class="col-sm-6">
                    <script>
                        document.write(new Date().getFullYear());
                    </script>
                    &copy; Nest - HTML Ecommerce Template .
                    <script>


                        document.addEventListener('DOMContentLoaded', function () {
                            let searchInput = document.getElementById('searchInput');

                            if (searchInput) { // Check if search input exists
                                searchInput.addEventListener('keyup', function () {
                                    let searchTerm = searchInput.value.toLowerCase();
                                    let tableRows = document.querySelectorAll('#userTable tbody tr');

                                    tableRows.forEach(function (row) {
                                        let nameColumn = row.querySelector('td:nth-child(2)'); // Assuming name is in the second column

                                        if (nameColumn) {
                                            let nameText = nameColumn.textContent.toLowerCase();
                                            row.style.display = nameText.includes(searchTerm) ? '' : 'none';
                                        }
                                    });
                                });
                            } else {
                                console.error("Search input element with ID 'searchInput' not found");
                            }
                        });


                    </script>
