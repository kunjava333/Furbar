                </ul>
            </div>
        </header>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products</h2>
                    <% if (typeof message !=="undefined" ) { %>
                        <p style="color: rgb(255, 0, 0);">
                            <%= message %>
                        </p>
                        <% } %>

                </div>
                <div>
                    <a href="#" class="btn btn-light rounded font-md">Export</a>
                    <a href="#" class="btn btn-light rounded font-md">Import</a>
                    <a href="/admin/add-product" class="btn btn-primary btn-sm rounded">Create new</a>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" id="searchInput" onkeyup="search()"  placeholder="Search..." class="form-control" />
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <form>

                                <select class="form-select" name="category" id="categorySelect"
                                    onchange="categoryData()" id="choose" required>

                                    <% if (typeof category !=="undefined" ) { %>

                                        <% category.forEach(element=> { %>
                                            <option id="option">
                                                <%= element.name %>
                                            </option>

                                            <% }); %>

                                                <% } %>

                                </select>
                            </form>
                        </div>

                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Latest added</option>
                                <option>Cheap first</option>
                                <option>Most viewed</option>
                            </select>
                        </div>
                    </div>

                </header>
                <!-- card-header end// -->
                <div class="container">
                    <div id="container" class="row row-cols-1 row-cols-md-4 g-4">
                           
                           
                    </div>
                  
            </div>


        </section>

