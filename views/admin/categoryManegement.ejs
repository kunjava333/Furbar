            <section class="content-main">
                <div class="content-header">
                    <div>
                        <h2 class="content-title card-title">Categories</h2>
                    </div>
                    <div>
                        <input type="text" placeholder="Search Categories" class="form-control bg-white" />
                    </div>
                </div>
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            
                            <div class="col-md-9">
                                <div class="table-responsive">
                                    <table class="table table-hover">
                                        <thead>
                                            <tr>
                                                <th class="text-center">
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="" />
                                                    </div>
                                                </th>
                                               
                                                <th>Name</th>
                                                <th>Description</th>
                                               
                                                
                                                <th class="text-end">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
<% if (typeof dbData !== "undefined") { %>
    

                                            <% dbData.forEach((element)=>{ %>
                                              
                                            <tr>
                                                <td class="text-center">
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="" />
                                                    </div>
                                                 
                                                </td>
                                                
                                                <td><b>
                                                   
                                                        <p><%= element.name %></p>
                                                       
                                                </b></td>
                                                <td><b>
                                                    <%= element.description %>
                                                </b></td>
                                               
                                             
                                                <td class="text-end">
                                                    <div class="dropdown">
                                                        <a href="/admin/edit-category?id=<%= element._id %>" class="btn btn-sm font-sm rounded btn-primary"  >Edit info</a>


                                                        
                                                <% if (element.isBlocked ==  true) {  %>
                                                    <a href="/admin/list/<%= element._id %>"  class="btn btn-sm font-sm  rounded btn-success" onclick="">  List </a>
                                              <%  } else{ %>
                                                        <a href="/admin/unlist/<%= element._id %>" class="btn btn-sm font-sm  rounded btn-danger" onclick="">  Unlist </a>
                                                
                                                      <% } %>
                                                
                                                        <a href="/admin/delete-category?id=<%= element._id %>" class="btn btn-sm font-sm btn-danger"  >Delete category</a>
                                                         
                                                    
                                 
                                                       
                                                    </div>
                                                    <!-- dropdown //end -->
                                                </td>
                                            </tr>
                                       
                                                
                                         
                                        </tbody>
                                        <% }) %>
                                        <% } %>
                                    </table>
                                </div>
                                <div>
                        
                        <a href="/admin/addcategory" class="btn btn-primary btn-sm rounded mt-2">Create new Category</a>
                    </div>
                            </div>
                            <!-- .col// -->
                        </div>
                        <!-- .row // -->
                    </div>
                    <!-- card body .// -->
                </div>
                <!-- card .// -->
            </section>
            <!-- content-main end// -->
            <footer class="main-footer font-xs">
